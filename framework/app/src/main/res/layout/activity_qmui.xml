<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.jiaxufei.framework.MainActivity">

    <com.qmuiteam.qmui.widget.QMUITopBar
        android:id="@+id/qmui_topbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/qmui_topbar_height"
        app:qmui_topbar_bg_color="@color/qmui_config_color_blue"
        app:qmui_topbar_left_back_drawable_id="@drawable/iv_back"
        app:qmui_topbar_need_separator="true"
        app:qmui_topbar_separator_color="@color/color_red"
        app:qmui_topbar_separator_height="2dp"
        app:qmui_topbar_title_gravity="left_center" />

    <Button
        android:id="@+id/btn_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dialog" />

    <com.qmuiteam.qmui.widget.textview.QMUILinkTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="15520727654  fsdfsdfsdd  jiaxufei@qq.com" />

    <com.qmuiteam.qmui.widget.grouplist.QMUIGroupListView
        android:id="@+id/groupListView"
        android:layout_width="wrap_content"
        android:layout_height="match_parent">

        <com.qmuiteam.qmui.widget.grouplist.QMUICommonListItemView
            android:id="@+id/itemView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:qmui_orientation="vertical" />
        <com.qmuiteam.qmui.widget.grouplist.QMUICommonListItemView
            android:id="@+id/itemView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:qmui_orientation="vertical" />
    </com.qmuiteam.qmui.widget.grouplist.QMUIGroupListView>
</LinearLayout>